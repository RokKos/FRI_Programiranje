fun putInt(n:int):void;
fun putChar(c:char):void;
fun putString(s:ptr char):void;
fun main():int =
    {
      putString("facItr:"); 
      putChar((10:char));
      n=1;
      while n<=10 do
        putInt(n);
        putString(": ");
		putInt(facItr(n));
		putChar((10:char));
		n=n+1;
      end;
      : 0
    };

fun facItr(n:int):int =
    { f=1;
      while n>0 do f=f*n; n=n-1; end;
    : f
    where
      var f:int;
    };